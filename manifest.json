{
    "name": "Blurify",
    "version": "0.1",
    "description": "blur your personal data with just one click",
    "manifest_version": 2,

    "browser_action": {
        "default_popup": "blurify.html"
    },
    "permissions": [
        "activeTab",
        "http://*/*",
        "https://*/*"
    ]
}